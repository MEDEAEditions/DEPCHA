    <text>
        <body>
            <div type="example">
                <head><name ana="bk:to" ref="#economic_unit.1">Stagville</name> August 1st
                    1808</head>
                <table>
                    <head>69 <name ana="bk:from" ref="#economic_unit.2">James Haley</name> J Self
                            <span ana="bk:debit"><choice>
                                <abbr>Dr.</abbr>
                                <expan>Debitor</expan>
                            </choice></span></head>
                    <row ana="bk:entry">
                        <cell>
                            <measure>1/4 lb Powder</measure>
                            <measure>1 lb Shot</measure>
                            <measure>1 lb Sugar</measure>
                        </cell>
                        <cell/>
                        <cell>
                            <measure>7</measure>
                        </cell>
                        <cell>
                            <measure>6</measure>
                        </cell>
                    </row>
                </table>
            </div>
        </body>
