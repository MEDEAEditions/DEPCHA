                        <name ana="bk:from">C. L. Hutchinson</name>
                        <measure ana="bk:money" quantity="100" unit="dollar">100</measure><span
                            ana="bk:status">Pd.</span></span>&lt;br /&gt;<span ana="bk:entry"><date
                            ana="bk:when" when="1895-08-06">Aug 6"</date>
