   <text>
      <body>
         <div>
            <table>
               <head>
                  <date ana="bk:when" when="1895-01-01">Jan'y 1st 1895</date>We the undersigned
                  hereby agree to give the sums set opposite our respective names for the current
                  expenses of <name ref="#HH">Hull House</name>.</head>
               <row ana="bk:entry">
                  <cell><date ana="bk:when" when="1895-01-01">Jan 1</date></cell>
                  <cell><name ana="bk:from" ref="#person.1">William Colvin</name></cell>
                  <cell><measure ana="bk:money" quantity="500" unit="dollar">500</measure></cell>
                  <cell ana="bk:status">pd.</cell>
               </row>
               <row ana="bk:entry">
                  <cell><date ana="bk:when" when="1895-06-02">June 2 "</date>
                  </cell>
                  <cell><name ana="bk:from">Mrs Barbara Armour</name></cell>
                  <cell>
                     <measure ana="bk:money" quantity="300" unit="dollar">300</measure>
                  </cell>
                  <cell ana="bk:status">pd.</cell>
               </row>
               <row ana="bk:entry">
                  <cell><date ana="bk:when" when="1895-06-06">June 6 "</date>
                  </cell>
                  <cell><name ana="bk:from">Seymour Coman</name></cell>
                  <cell>
                     <measure ana="bk:money" quantity="200" unit="dollar">200</measure>
                  </cell>
                  <cell ana="bk:status">pd.</cell>
               </row>
               <row ana="bk:entry">
                  <cell><date ana="bk:when" when="1895-06-18">June 18 "</date>
                  </cell>
                  <cell><name ana="bk:from">John R. Walsh</name></cell>
                  <cell>
                     <measure ana="bk:money" quantity="200" unit="dollar">$200</measure>
                  </cell>
                  <cell ana="bk:status">pd.</cell>
               </row>
               <row ana="bk:entry">
                  <cell><date ana="bk:when" when="1895-10-14">Oct 14 "</date></cell>
                  <cell><name ana="bk:from">L. J. Gage</name></cell>
                  <cell>
                     <measure ana="bk:money" quantity="100" unit="dollar">100</measure></cell>
                  <cell ana="bk:status">pd.</cell>
               </row>
               <row ana="bk:entry">
                  <cell><date ana="bk:when" when="1895-06-17">June 17 "</date>
                  </cell>
                  <cell><name ana="bk:from">Mrs George E. Adams</name></cell>
