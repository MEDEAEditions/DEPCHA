      <bk:Transfer rdf:about="https://gams.uni-graz.at/o:depcha.1#T271T2">
        [...]
      </bk:Transfer>
      <bk:Transfer rdf:about="https://gams.uni-graz.at/o:depcha.1#T271T3">
