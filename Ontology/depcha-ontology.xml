    <rdfs:range rdf:resource="https://gams.uni-graz.at/o:depcha.bookkeeping#TypeAssignment"></rdfs:range>
    <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P42_assigned"></rdfs:subPropertyOf>
    <skos:example rdf:parseType="Literal">
      [...]
      <t:taxonomy ana="bk:assigned">
        <t:gloss>Farms</t:gloss>
        <t:category xml:id="farm_1">
          <t:gloss>Fairfax Farm</t:gloss>
        </t:category>
        <t:category xml:id="farm_2">
          <t:gloss>Williams Farm</t:gloss>
        </t:category>
      </t:taxonomy>
      [...]
      <t:div ana="bk:entry">
        To Cash paid for <t:span ana="bk:assigned" ref="#farm_1">Fairfax Farm</t:span>
        and <t:span ana="bk:assigned" ref="#farm_2">Williams Farm</t:span>
      </t:div>
    </skos:example>
    <skos:example rdf:parseType="Literal">
      <bk:TypeAssignment rdf:about="https://gams.uni-graz.at/o:depcha.1#farm_1">
        [...]
      </bk:TypeAssignment>
    </skos:example>
  </rdf:Property>
  <rdf:Property rdf:about="https://gams.uni-graz.at/o:depcha.bookkeeping#by">
    <rdfs:label xml:lang="en">by</rdfs:label>
    <rdfs:label xml:lang="de">bei</rdfs:label>
